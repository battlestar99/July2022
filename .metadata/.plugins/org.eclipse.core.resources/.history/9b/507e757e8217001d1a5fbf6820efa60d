package samplePackage;

public class LoopLearning {

	public static void main(String[] args) {

		
		int a = 1;
		System.out.println(a);
		
//		if(a<=10) {
//			System.out.println("a is less than 10");
//		}
		
//		System.out.println("Mehdi");
//		System.out.println("Mehdi");
//		System.out.println("Mehdi");
