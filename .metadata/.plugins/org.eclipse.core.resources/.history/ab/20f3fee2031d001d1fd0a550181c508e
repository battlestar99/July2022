package samplePackage;

public class FunctionLearning {

	public static void main(String[] args) {
				
		FunctionLearning fl = new FunctionLearning();
		fl.addingIntNumbers();
		fl.addingIntNumbers();
		fl.addingIntNumbers();
		
	}
	
	public void addingIntNumbers() {
		int a = 10;
		int b = 20;
		int c = a + b;
		System.out.println(c);
	}
	
}







