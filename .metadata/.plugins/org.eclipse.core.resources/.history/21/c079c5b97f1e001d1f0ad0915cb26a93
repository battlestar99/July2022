package samplePackage;

public class FunctionLearning {

	public static void main(String[] args) {
		
		for(int i = 1; i<=100; i++) {
			System.out.print(i);
			if(i%10 == 0) {
				System.out.println();
			}
		}
		
		
		
//		FunctionLearning fl = new FunctionLearning();
