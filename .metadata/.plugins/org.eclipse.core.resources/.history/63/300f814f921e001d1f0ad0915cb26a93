package functions;

public class AddingIntNumbers {

	public int addingIntTwoNumbers(int a, int b) {
		int c = a + b;
		return c;
	}
	
	public int addingIntThreeNumbers(int a, int b, int c) {
		int d = a + b + c;
		System.out.println(d);
		return d;
	}
	
	public void addintNumbers(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}
}
