package samplePackage;

public class ConditionalStatement {

	public static void main(String[] args) {
		
		int a = 99;
		
		
		if(a <= 100) {
			System.out.println("a value is less than or equals to 100");
		}else {
			System.out.println("a value is not less than 100");
		}

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
