package samplePackage;

public class ConditionalStatement {

	public static void main(String[] args) {
		
		int a = 99;
		
		
		if(a <= 100) {
			System.out.println("a value is less than or equals to 100");
		}else {
			System.out.println("a value is not less than 100");
		}

		// >=
		
		// declare a variable b initialize with 100
		// check if the value is greater or equals to 50 -- b 
		// 100 is greater than or equals to b
		
		int b = 100;
		
		if(b != 50) {
			System.out.println("b value is not equals to 50");
		}else {
			System.out.println("b value is equals to 50");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
