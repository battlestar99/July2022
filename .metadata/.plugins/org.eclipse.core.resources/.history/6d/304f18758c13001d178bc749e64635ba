package samplePackage;

public class ArrayLearning {

	public static void main(String[] args) {
		
		int a;
		a = 10;
		a = 100;
		
		
		int[] b = {10, 20, 30};
		
		int[] c = new int[5];
		
		c[0] = 1000;
		c[1] = 100;
		c[2] = 200;
		c[3] = 300;
		c[4] = 400;
		
		
		
		String[] names = {"Dee", "Syed", "Ali"};
		
		
		
		
		
		
		
		
		System.out.println(a);
		
		a = 500;
		
		System.out.println(a);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
